//@execute 0=false; 1=true; 2=true; 3=false
type P(x: int, y: int) { }
def main(arg: int) -> bool {
	var a: P;
	var eq = equals;
	if (arg == 1) return eq(P(arg, 1), P(arg, 1));
	if (arg == 2) return eq(a, P(0, 0));
	if (arg == 3) return eq(P(4, arg), P(arg, 3));
	return false;
}
def equals(a: P, b: P) -> bool {
	return a == b;
}

